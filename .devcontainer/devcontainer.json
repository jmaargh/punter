{
  "name": "Punter (Ubuntu)",
  "dockerFile": "Dockerfile",
  "runArgs": [
    "-v",
    "${env:HOME}/.ssh/:/home/dev/.ssh:ro"
  ],
  "appPort": 1313,
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "rust-lang.rust",
    "bungcip.better-toml",
    "yzhang.markdown-all-in-one",
    "davidanson.vscode-markdownlint"
  ]
}